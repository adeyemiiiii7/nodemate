{
  "name": "nodemate",
  "version": "1.0.0",
  "description": "AI-powered CLI assistant for Node.js package management supporting Express, NestJS, and vanilla Node.js projects with multiple LLM providers",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "nodemate": "./bin/nodemate.js",
    "nm": "./bin/nodemate.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "cli",
    "nodejs",
    "npm",
    "package-manager",
    "ai",
    "assistant",
    "express",
    "nestjs",
    "llm",
    "openai",
    "claude",
    "gemini"
  ],
  "author": "Dr. Node Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.9.2",
    "@types/jest": "^29.5.12",
    "@types/fs-extra": "^11.0.4",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "groq-sdk": "^0.34.0",
    "inquirer": "^12.10.0",
    "openai": "^6.7.0",
    "ora": "^9.0.0",
    "semver": "^7.6.0"
  }
}
